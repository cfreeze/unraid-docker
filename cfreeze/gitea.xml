<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Name>gitea</Name>
  <Description>
Git with a cup of tea
  </Description>
  <Overview>
Git SCM server
  </Overview>
  <GitHub>hhttps://github.com/cfreeze/unraid-docker</GitHub>
  <Repository>gitea/gitea</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>2200</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
	  <Port>
        <HostPort>3000</HostPort>
        <ContainerPort>3000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
  </Environment>    
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:3000]/</WebUI>
  <Banner></Banner>
  <Icon>https://gitea.io/images/gitea.png</Icon>
  <Beta>False</Beta>
  <Category>Other: Development:</Category>
  <Date>2017-04-22</Date>
  <Changes>
    [font size=4]2017.04.22[/font]
    - Initial Release    
  </Changes>
</Containers>