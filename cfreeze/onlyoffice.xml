<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Name>OnlyOffice</Name>
  <Description>
OnlyOffice Community Document Server
  </Description>
  <Overview>
 OnlyOffice Community Document Server
  </Overview>
  <GitHub>hhttps://github.com/cfreeze/unraid-docker</GitHub>
  <Repository>onlyoffice/communityserver</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>80</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
	  <Port>
        <HostPort>443</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
  </Environment>    
  <Data>
    <Volume>
      <HostDir>/mnt/users/onlyoffice</HostDir>
      <ContainerDir>/var/www/onlyoffice/Data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <Banner></Banner>
  <Icon>http://i.ytimg.com/vi/RniZtw0A9WQ/hqdefault.jpg</Icon>
  <Beta>False</Beta>
  <Category>Cloud: Office:</Category>
  <Date>2017-04-23</Date>
  <Changes>
    [font size=4]2017.04.23[/font]
    - Initial Release    
  </Changes>
</Containers>